<%

    include("../logic/locationLogic.jag");

    var verb= request.getMethod();
    var id= request.getParameter('id');

    var uri = request.getPathInfo();
    if(uri != null) {
    var uriMatcher = new URIMatcher(uri);
        if(uriMatcher.match("/{page}/{id}")){
            id=uriMatcher.elements().id;
        }
    }
    if(verb=="GET"){
        getLocation(id);
    }

%>